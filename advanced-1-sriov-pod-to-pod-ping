apiVersion: sriovnetwork.openshift.io/v1
kind: SriovNetwork
metadata:
  name: nadens7f1-1
  namespace: openshift-sriov-network-operator 
spec:
  resourceName: nicens7f1
  networkNamespace: test-sriov-ns
  ipam: '{ "type": "static", "addresses": [{ "address": "192.168.133.1/24" }]}'
---
apiVersion: sriovnetwork.openshift.io/v1
