  networkNamespace: test-sriov-ns
  ipam: '{ "type": "static", "addresses": [{ "address": "192.168.133.2/24" }]}'
---
apiVersion: v1
kind: Pod
metadata:
  labels:
    run: testpod
  name: pod1-using-sriov
  namespace: test-sriov-ns
  annotations:
          k8s.v1.cni.cncf.io/networks: nadens7f1-1
spec:
  containers:
  - image: quay.io/sfhassan/newtools
    name: testpod
